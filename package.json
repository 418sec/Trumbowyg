{
	"name": "trumbowyg",
	"title": "Trumbowyg",
	"description": "A lightweight WYSIWYG editor",
	"version": "3.0.0-alpha.0",
	"private": true,
	"main": "dist/trumbowyg.js",
	"scripts": {
		"test": "NODE_ENV=test TS_NODE_PROJECT='tsconfig.tests.json' ava"
	},
	"ava": {
		"files": [
			"tests/**"
		],
		"extensions": [
			"ts"
		],
		"require": [
			"tsconfig-paths/register",
			"ts-node/register",
			"source-map-support/register"
		]
	},
	"devDependencies": {
		"ava": "^3.13.0",
		"source-map-support": "^0.5.19",
		"ts-node": "^9.0.0",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.0.3"
	},
	"homepage": "http://alex-d.github.io/Trumbowyg",
	"author": {
		"name": "Alexandre Demode (Alex-D)",
		"email": "contact@alex-d.fr",
		"url": "http://alex-d.fr"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Alex-D/Trumbowyg.git"
	},
	"bugs": {
		"url": "https://github.com/Alex-D/Trumbowyg/issues"
	},
	"license": "MIT",
	"keywords": [
		"wysiwyg",
		"editor",
		"rich text",
		"contenteditable",
		"trumbowyg"
	]
}
